<!doctype html{% block html_start %}{% endblock %}>
<html lang="{{ app.request.locale }}"{% if tabler_bundle.isRightToLeft() %} dir="rtl"{% endif %}>
<head>
    {% block head %}
        {{ include('@Tabler/includes/html_head.html.twig', {viewport_cover: true}) }}
    {% endblock %}
    <title>{% block title %}{% endblock %}</title>
    {% block stylesheets %}
        {{ include('@Tabler/includes/css_assets.html.twig') }}
    {% endblock %}
</head>
<body{% block body_start %}{% endblock %} class="d-flex flex-column {{ 'border-top-wide border-primary theme-light'|tabler_body }} {% block body_class %}{% endblock %}" data-bs-theme="{{ tabler_theme() }}">
{% block after_body_start %}{% endblock %}

<div class="{% block page_classes %}page page-center{% endblock %}">
    {% block page_content %}{% endblock %}
</div>

{% block javascripts %}
    {{ include('@Tabler/includes/js_assets.html.twig') }}
{% endblock %}
</body>
</html>
